{
    "hosting": {
  
    "headers": [ 
      {
      "source": "**",
      "headers": [ 
        {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      } 
    ]
    } ],
  
      
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  }
  